{
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Test",
            "connectUnderReset": true,
            "chip": "STM32U575ZITxQ",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": true
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "stm32u5/target/thumbv8m.main-none-eabihf/debug/adc_raw"
                }
            ]
        },
        {
            "cwd": "${workspaceFolder}/stm32u5",
            "preLaunchTask": "build debug",
            "executable": "./target/thumbv8m.main-none-eabihf/debug/app",
            "name": "Debug App",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "stlink"
        },
        {
            "cwd": "${workspaceFolder}/stm32u5",
            "preLaunchTask": "build release",
            "executable": "./target/thumbv8m.main-none-eabihf/release/app",
            "name": "Debug App Release",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "stlink"
        },
        {
            "cwd": "${workspaceFolder}/stm32u5",
            //   "preLaunchTask": "build debug",
            "executable": "./target/thumbv8m.main-none-eabihf/debug/adc_raw",
            "name": "ADC",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "servertype": "stlink"
        }
    ]
}